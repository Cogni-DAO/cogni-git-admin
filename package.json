{
  "name": "{{ name }}",
  "version": "1.0.0",
  "type": "module",
  "description": "{{{ description }}}",
  "author": "{{{ author }}}",
  "license": "ISC",
  "repository": "https://github.com/{{ owner }}/{{ repo }}.git",
  "homepage": "https://github.com/{{ owner }}/{{ repo }}",
  "bugs": "https://github.com/{{ owner }}/{{ repo }}/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc -p tsconfig.runtime.json && cp shim/* dist/",
    "dev": "nodemon --exec \"npm start\"",
    "dev:capture": "tsx tools/dev/webhook-capture/capture-server.ts",
    "smee-capture-chain-events": "npx dotenv -- sh -c 'smee -u $ALCHEMY_PROXY_URL --path /capture --port 4001'",
    "smee-capture-git-events": "npx dotenv -- sh -c 'smee -u $WEBHOOK_PROXY_URL --path /capture --port 4001'",
    "capture": "concurrently \"npm run smee-capture-chain-events\" \"npm run smee-capture-git-events\"",
    "start": "probot run dist/runtime.js",
    "lint": "eslint \"{src,test}/**/*.{ts,tsx,js}\"",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage",
    "e2e": "npx dotenv -- npx playwright test",
    "e2e:fixtures": "npx dotenv -- npx playwright test --project=fixture-replay",
    "e2e:blockchain": "npx dotenv -- npx playwright test --project=blockchain-e2e",
    "validate-e2e-setup": "npx dotenv -- tsx tools/dev/chain-setup/validate-setup.ts",
    "deploy-test-dao": "npx dotenv -- tsx tools/dev/chain-setup/deploy-test-contracts.ts"
  },
  "dependencies": {
    "@octokit/plugin-retry": "^8.0.2",
    "@octokit/plugin-throttling": "^11.0.2",
    "@octokit/request-error": "^7.0.1",
    "@octokit/rest": "^22.0.0",
    "@octokit/types": "^15.0.0",
    "octokit": "^5.0.3",
    "probot": "^7.0.0",
    "viem": "^2.38.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@playwright/test": "^1.56.0",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^10.5.2",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "@viem/anvil": "^0.0.10",
    "concurrently": "^8.2.2",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-typescript": "^0.12.0",
    "globals": "^16.4.0",
    "jest": "^30.2.0",
    "mkdirp": "^3.0.1",
    "nanoid": "^5.1.6",
    "nock": "^14.0.10",
    "nodemon": "^1.17.2",
    "prettier": "^3.6.2",
    "smee-client": "^1.0.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.0"
  },
  "engines": {
    "node": ">=20.8"
  }
}
