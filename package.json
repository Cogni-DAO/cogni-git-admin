    "@typescript-eslint/parser": "^8.46.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-typescript": "^0.12.0",
    "globals": "^16.4.0",
    "jest": "^30.2.0",
    "nodemon": "^1.17.2",
    "prettier": "^3.6.2",
    "smee-client": "^1.0.2",
    "ts-jest": "^29.4.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.0"
